<tbody>
<% result.forEach(function(data){ %>
<tr>
    <td style="width: 5px;">
        <button class="btn-view-fund btn btn-default btn-xs pull-left" type="button"
                data-toggle="modal" data-target="#modifyTimerModel">
            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
        </button>
    </td>
    <td style="width: 5px;">
        <button class="btn-view-fund btn btn-default btn-xs pull-left" type="button"
                data-toggle="modal" data-target="#deleteTimerModel">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </button>
    </td>
    <td style="width: 5px;">
        <button class="btn-view-fund btn btn-default btn-xs pull-left" type="button">
            <span class="glyphicon glyphicon-play" aria-hidden="true"></span>
        </button>
    </td>
    <td style="width: 5px;">
        <button class="btn-view-fund btn btn-default btn-xs pull-left" type="button">
            <span class="glyphicon glyphicon-pause" aria-hidden="true"></span>
        </button>
    </td>
    <td style="text-align: left;"><%- data.timer_description %>:
        <%- data.timer_total.hours %>:<%- data.timer_total.minutes %>
        (<%- data.timer_interval.hours %>'<%- data.timer_interval.minutes %>"/d)
    </td>
</tr>
<% }); %>
</tbody>
